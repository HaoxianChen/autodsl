.decl recvRERR(x0:symbol,x1:number)
.decl route(x0:symbol,x1:symbol,x2:number)
.decl this(x0:symbol,x1:number)
.decl delRoute(x0:symbol,x1:symbol,x2:number)
.input recvRERR
.input route
