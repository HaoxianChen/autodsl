_new_route(node4,node1,node0,cost2,instanceid0) :- cost0 != cost1,link(_,node0,cost0,instanceid0),this(node4,instanceid0),cost2=cost1+cost0,route(node0,node1,_,cost1,instanceid0).
minCost(node1,instanceid0,min_cost) :- _new_route(_,node1,_,_,instanceid0),min_cost = min c : _new_route(_,node1,_,c,instanceid0).
new_route(node0,node1,node2,cost0,instanceid0) :- _new_route(node0,node1,node2,cost0,instanceid0),minCost(node1,instanceid0,cost0).
_new_route(node0,node2,node5,cost1,instanceid0) :- this(node0,instanceid0),_new_route(_,node2,node3,_,instanceid0),route(node4,node2,node5,cost1,instanceid0),link(node4,node3,_,instanceid0).