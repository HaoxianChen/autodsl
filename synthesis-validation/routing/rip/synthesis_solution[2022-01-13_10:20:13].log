_new_route(node0,node1,node2,cost0,instanceid0) :- route(node0,node1,node2,cost0,instanceid0),link(node0,_,_,instanceid0).
_new_route(node0,node4,node6,cost2,instanceid0) :- this(node0,instanceid0),node4 != node2,cost0 != cost3,link(node5,node6,cost1,instanceid0),cost2=cost0+cost1,_new_route(node5,node4,node3,cost3,instanceid0),route(_,node2,node3,cost0,instanceid0).
minCost(node1,instanceid0,min_cost) :- _new_route(_,node1,_,_,instanceid0),min_cost = min c : _new_route(_,node1,_,c,instanceid0).
new_route(node0,node1,node2,cost0,instanceid0) :- _new_route(node0,node1,node2,cost0,instanceid0),minCost(node1,instanceid0,cost0).
_new_route(node0,node2,node1,cost2,instanceid0) :- link(node0,node1,cost0,instanceid0),route(node1,node2,_,cost1,instanceid0),cost2=cost1+cost0,_new_route(_,_,node1,cost2,instanceid0).