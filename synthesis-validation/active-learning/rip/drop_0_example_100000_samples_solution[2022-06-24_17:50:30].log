_new_route(node0,node1,node2,cost0,instanceid0) :- this(node0,instanceid0),route(node0,node1,node2,cost0,instanceid0),!link(node0,_,_,instanceid0).
_new_route(node0,node1,node2,cost0,instanceid0) :- this(node3,instanceid0),node4 != node0,link(node3,node4,_,instanceid0),route(node0,node1,node2,cost0,instanceid0),_new_route(node0,_,_,_,instanceid0).
_new_route(node0,node2,node1,cost2,instanceid0) :- this(node0,instanceid0),node0 != node1,link(node0,node1,cost1,instanceid0),route(node1,node2,_,cost0,instanceid0),cost2=cost0+cost1.
new_route(node0,node1,node2,cost0,instanceid0) :- _new_route(node0,node1,node2,cost0,instanceid0),minCost(node1,instanceid0,cost0).
minCost(node1,instanceid0,min_cost) :- _new_route(_,node1,_,_,instanceid0),min_cost = min c : _new_route(_,node1,_,c,instanceid0).