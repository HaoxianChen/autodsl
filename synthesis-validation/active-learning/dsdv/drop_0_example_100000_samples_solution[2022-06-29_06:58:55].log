newRoute(node1,distance0,node0,seq1,instanceid0) :- brokenLink(node0,instanceid0),infinity(distance0,instanceid0),route(node1,_,node0,seq0,instanceid0),seq1=seq0+1.
newRoute(node3,distance2,node2,seq1,instanceid0) :- distance0 > distance1,route(node0,distance0,node1,seq0,instanceid0),!seq0 > seq1,distance2=distance1+1,node1 != node0,recvUpdate(node2,node3,distance1,seq1,instanceid0).
newRoute(node1,distance3,node0,seq0,instanceid0) :- !brokenLink(node2,instanceid0),distance3=distance0+1,route(node2,distance1,_,seq1,instanceid0),!distance1 > distance0,seq0 != seq1,infinity(_,instanceid0),recvUpdate(node0,node1,distance0,seq0,instanceid0).