canSshTunnel(node2,node0,instanceid0) :- canSsh(node0,_,instanceid0),canSsh(node2,node0,instanceid0).
canSshTunnel(node0,node2,instanceid0) :- canSshTunnel(node0,node1,instanceid0),canSsh(node1,node2,instanceid0),node0 != node2.
canSshTunnel(node1,node3,instanceid0) :- canSsh(node0,node1,instanceid0),canSshTunnel(node0,node2,instanceid0),canSsh(node2,node3,instanceid0).