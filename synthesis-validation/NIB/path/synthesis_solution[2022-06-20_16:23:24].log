Path(node0,node1,nodelist0,instanceid0) :- Link(node0,node1,instanceid0),nodelist0=cat(node0,node1),node0 != node1.
Path(node0,node2,nodelist1,instanceid0) :- Path(node0,node1,nodelist0,instanceid0),Link(node1,node2,instanceid0),node2 != node0,nodelist1=cat(nodelist0, node2), !contains(node2, nodelist0).