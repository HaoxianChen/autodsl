Path(node0,node1,nodelist0,instanceid0) :- Link(node0,node1,instanceid0),nodelist0=cat(node0,node1),node1 != node0.
Path(node2,node1,nodelist1,instanceid0) :- Link(node0,node1,instanceid0),Path(node2,node0,nodelist0,instanceid0),nodelist1=cat(nodelist0, node1), !contains(node1, nodelist0),node1 != node2.