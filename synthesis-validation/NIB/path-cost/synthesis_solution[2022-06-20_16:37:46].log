Path(node0,node1,nodelist0,cost0,instanceid0) :- Link(node0,node1,cost0,instanceid0),nodelist0=cat(node0,node1),node0 != node1.
Path(node0,node2,nodelist1,cost3,instanceid0) :- Path(node1,node2,nodelist0,cost1,instanceid0),cost3=cost0+cost1,nodelist1=cat(node0, nodelist0), !contains(node0, nodelist0),node2 != node3,Link(node3,node2,_,instanceid0),Link(node0,node1,cost0,instanceid0).